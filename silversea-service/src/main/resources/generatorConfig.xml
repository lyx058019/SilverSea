<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd"><generatorConfiguration>    <!-- 数据库驱动包位置 -->    <!--<classPathEntry location="D:\web\java\generator\mysql-connector-java-5.1.34.jar" />-->    <!--<classPathEntry location="/Users/liuyanxin/Work/WorkSoft/MvnRepository/mysql/mysql-connector-java/8.0.13/mysql-connector-java-8.0.13.jar"></classPathEntry>-->    <classPathEntry location="D:\workSoft\apache-maven-3.6.0\store\mysql\mysql-connector-java\8.0.13\mysql-connector-java-8.0.13.jar"></classPathEntry>    <!-- <classPathEntry location="C:\oracle\product\10.2.0\db_1\jdbc\lib\ojdbc14.jar" />-->    <context id="DB2Tables" targetRuntime="MyBatis3">        <commentGenerator>            <property name="suppressAllComments" value="true" />        </commentGenerator>        <!-- 数据库链接URL、用户名、密码 -->        <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://localhost:3306/silverSea?serverTimezone=GMT%2B8" userId="root" password="123456">            <!-- 这里面可以设置property属性，每一个property属性都设置到配置的Driver上 -->        </jdbcConnection>        <javaTypeResolver>            <property name="forceBigDecimals" value="false" />        </javaTypeResolver>        <!-- 生成模型的包名和位置 -->        <javaModelGenerator targetPackage="com.lyx.silversea.model" targetProject="src/main/java">            <property name="enableSubPackages" value="true" />            <property name="trimStrings" value="true" />        </javaModelGenerator>        <!-- 生成的映射文件包名和位置 -->        <sqlMapGenerator targetPackage="mapper" targetProject="src/main/resources">            <property name="enableSubPackages" value="true" />        </sqlMapGenerator>        <!-- 生成DAO的包名和位置 -->        <javaClientGenerator type="XMLMAPPER" targetPackage="com.lyx.silversea.dao" targetProject="src/main/java">            <property name="enableSubPackages" value="true" />        </javaClientGenerator>        <!-- 要生成那些表(更改tableName和domainObjectName就可以) -->        <table tableName="role" domainObjectName="role" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false" />        <!-- <table tableName="course_info" domainObjectName="CourseInfo" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false" />        <table tableName="course_user_info" domainObjectName="CourseUserInfo" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false" /> -->    </context></generatorConfiguration>